{
  "dart.experimentalSidebarDevTools": true,
  "dart.devToolsLocation": {
    "inspector": "sidebar",
    "cpu-profiler": "external",
    "default": "beside"
  },
  "[dart]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "editor.selectionHighlight": false,
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.links": true,
    "editor.rulers": [
      120
    ]
  },
  "dart.lineLength": 120,
  "dart.doNotFormat": [
    "**.g.dart",
    "**.gql.dart",
    "**.freezed.dart",
    "**.config.dart",
    "**.mocks.dart",
    "**.gen.dart",
    "**.pb.dart",
    "**.pbenum.dart",
    "**.pbjson.dart",
    "**/generated/**"
  ],
  "dart.additionalAnalyzerFileExtensions": [
    "drift"
  ],
  "dart.flutterSdkPath": ".fvm/versions/stable",
  "dart.addSdkToTerminalPath": true,
  "[html]": {
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "search.exclude": {
    "**/.fvm": true,
    ".dart_tool": true,
    "coverage": true,
    "build": true
  },
  "files.watcherExclude": {
    "**/.fvm": true,
    ".dart_tool": true,
    "coverage": true,
    "build": true
  },
  "files.exclude": {
    ".old": true
  },
  "debug.openDebug": "openOnDebugBreak",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "pubspec.yaml": ".flutter-plugins, .packages, .dart_tool, .flutter-plugins-dependencies, .metadata, .packages, pubspec.lock, build.yaml, analysis_options.yaml, all_lint_rules.yaml, dart*.yaml, flutter*.yaml, icons_launcher.yaml, l10n.yaml, melos.yaml, pubspec_overrides.yaml, devtools_options.yaml, sentry.properties, codemagic.yaml",
    ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .sentryignore, .git-blame*, .*ignore",
    "readme.*": "authors, backers.md, changelog*, citation*, code_of_conduct.md, codeowners, contributing.md, contributors, copying, credits, governance.md, history.md, license*, maintainers, readme*, security.md, sponsors.md",
    "*.dart": "i_$(capture).dart, $(capture).g.dart, $(capture).gr.dart, $(capture).freezed.dart, $(capture).config.dart, $(capture).chopper.dart, $(capture).drift, $(capture)_io.dart, $(capture)_web.dart, $(capture)_shared.dart, $(capture)_stub.dart",
    "*.graphql": "$(capture).gql.dart, $(capture).*.gql.dart",
    "firebase.json": " .firebaserc",
    "main.dart": "runner_io.dart, runner_shared.dart, runner_stub.dart, runner_web.dart"
  },
  "highlight.regexes": {
    "(\"@\\s*.+\":\\s{0,1}{},)": {
      "filterFileRegex": ".*\\.arb",
      "decorations": [
        {
          "overviewRulerColor": "#d19a66",
          "backgroundColor": "#d19a66",
          "color": "#282c34",
          "fontWeight": "bold"
        }
      ]
    }
  },
  "cSpell.words": [
    "loadfragment"
  ],
  "cSpell.ignorePaths": [
    "lib/_core/localization/localization.dart"
  ],
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.preferredAccount": "zs-dima",
  "makefile.configureOnOpen": false,
  "github-actions.workflows.pinned.workflows": [
    ".github/workflows/deploy-firebase.yml"
  ],
  "cmake.ignoreCMakeListsMissing": true,
  "sonarlint.connectedMode.project": {
    "connectionId": "",
    "projectKey": ""
  },
  "dart.projectSearchDepth": 0
}